{
  "name": "react-htmlcontent",
  "description": "Lightweight component which displays HTML string, fixes common typographic issues, adds non-breaking spaces and sanitizes HTML.",
  "author": "Viktor Bezdek <viktor.bezdek@gmail.com>",
  "user": "viktorbezdek",
  "version": "1.0.0",
  "scripts": {
    "start": "webpack-dev-server --env dev",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:lint": "eslint . --ignore-path .gitignore --cache",
    "gh-pages": "webpack --env ghPages",
    "gh-pages:deploy": "gh-pages -d gh-pages",
    "gh-pages:stats": "webpack --env ghPages --profile --json > stats.json",
    "dist": "webpack --env dist",
    "dist:es6": "rimraf ./dist-es6 && BABEL_ENV=es6 babel ./src --out-dir ./dist-es6",
    "dist:min": "webpack --env distMin",
    "dist:modules": "rimraf ./dist-modules && BABEL_ENV=modules babel ./src --out-dir ./dist-modules",
    "preversion": "npm run test && npm run dist && npm run dist:min && git commit --allow-empty -am \"Update dist\"",
    "prepublish": "npm run dist:es6 && npm run dist:modules",
    "postpublish": "npm run gh-pages && npm run gh-pages:deploy",
    "postinstall": "node lib/post_install.js"
  },
  "main": "dist-modules",
  "module": "dist-es6",
  "jsnext:main": "dist-es6",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.6",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "catalog": "^2.5.3",
    "chai": "^4.0.2",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "eslint": "^4.1.1",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-loader": "^1.8.0",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^6.0.0",
    "eslint-plugin-react": "^7.1.0",
    "expect": "^1.20.2",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "gh-pages": "^1.0.0",
    "git-prepush-hook": "^1.0.2",
    "html-webpack-plugin": "^2.29.0",
    "html-webpack-template": "^6.0.1",
    "jest": "^20.0.4",
    "json-loader": "^0.5.4",
    "purecss": "^1.0.0",
    "raw-loader": "^0.5.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-github-corner": "^0.3.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "sync-exec": "^0.6.2",
    "system-bell-webpack-plugin": "^1.0.0",
    "url-loader": "^0.5.9",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0",
    "webpack-merge": "^4.1.0"
  },
  "peerDependencies": {
    "react": ">= 0.11.2 < 16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/viktorbezdek/react-htmlcontent.git"
  },
  "homepage": "https://viktorbezdek.github.io/react-htmlcontent/",
  "bugs": {
    "url": "https://github.com/viktorbezdek/react-htmlcontent/issues"
  },
  "jest": {
    "collectCoverage": true,
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "packages"
    ]
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate"
  ],
  "license": "MIT",
  "pre-push": [
    "test"
  ],
  "dependencies": {
    "htmlparser2": "^3.9.2",
    "prop-types": "^15.5.10",
    "regexp-quote": "^0.0.0",
    "xtend": "^4.0.1"
  }
}
